{
  "name": "test-task",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "api:start": "nest start --tsc --entryFile ./entry-points/http/api/main",
    "api:start:dev": "nest start --tsc --entryFile ./entry-points/http/api/main --watch",
    "api:start:debug": "nest start --tsc --entryFile ./entry-points/http/api/main --watch --debug",
    "notification:start": "nest start --tsc --entryFile ./entry-points/consumers/notification/main",
    "notification:start:dev": "nest start --tsc --entryFile ./entry-points/consumers/notification/main --watch",
    "notification:start:debug": "nest start --tsc --entryFile ./entry-points/consumers/notification/main --watch --debug",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "env": "docker compose -f docker-compose.env.yaml up",
    "start:docker": "docker compose -f docker-compose.local.yaml up --build",
    "migrate": "node-pg-migrate -j ts --tsconfig ./tsconfig.json",
    "migration:up": "./scripts/run-migration.sh up",
    "migration:down": "./scripts/run-migration.sh up",
    "test:unit": "jest --config ./test/jest-unit.json"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.750.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.0.3",
    "@types/pg": "^8.11.11",
    "nestjs-zod": "^4.3.1",
    "pg": "^8.13.3",
    "postgres": "^3.4.5",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "node-pg-migrate": "^7.9.1",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
